{
	"remote": {
		"host": "walmartv5.vtexcommercebeta.com.br",
		"port": 80
	},
	"proxy": {
		"host": "localhost",
		"port": 80
	},
	"server": {
		"host": "localhost",
		"port": 9001
	},
	"headers": {
		"X-VTEX-Environment": "beta"
	},
	"bounces": [
		"^/$",
		"/css/.*",
		"/img/.*",
		"/js/.*",
		"/bower_components/.*",
		"/templates/.*"
	],
	"mapping": true,
	"mappings": {
		"/api/postal/pub/address/BRA/": "test/mock/postalCode.json"
	}
}
