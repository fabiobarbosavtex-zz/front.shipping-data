{
	"remote": {
		"host": "walmartv5.vtexcommercebeta.com.br",
		"port": 80
	},
	"proxy": {
		"host": "localhost",
		"port": 80
	},
	"server": {
		"host": "localhost",
		"port": 9001
	},
	"headers": {
		"X-VTEX-Router-Backend-EnvironmentType": "beta"
	},
	"bounces": [
		"^/shipping-ui/$",
		"/shipping-ui/css/.*",
		"/shipping-ui/img/.*",
		"/shipping-ui/js/.*",
		"/shipping-ui/libs/.*",
		"/shipping-ui/templates/.*"
	],
	"mapping": false,
	"mappings": {
		"/api/postal/pub/address/BRA/": "test/mock/postalCode.json"
	}
}
